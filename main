#include <stdio.h>
//将五个字节的十六进制数转换成一个十进制数
//读取原始字节数据
char srcdata[5] = {};
char srcdec[20] = {};
int val;
int i;
char *p[] = (char*)&val;
printf("请输入五个十六进制数据：\n");
for(i=0;i<5;i++)
{
  scanf("%f\n",&srcdata[i]);
}
p[0] = srcdata[4];
p[1] = srcdata[3];
p[2] = srcdata[2];
p[3] = srcdata[1];
p[4] = srcdata[0];
sprintf(srcdec, "%d", val);
printf("%d", srcdec);
